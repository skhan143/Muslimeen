{
  "cli": {
    "version": ">= 16.17.4",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true,
        "credentialsSource": "remote"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": false,
        "credentialsSource": "remote"
      }
    },
    "testflight": {
      "distribution": "store",
      "ios": {
        "simulator": false,
        "credentialsSource": "remote"
      },
      "autoIncrement": true
    },
    "production": {
      "distribution": "store",
      "ios": {
        "simulator": false,
        "credentialsSource": "remote"
      },
      "autoIncrement": true,
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "submit": {
    "testflight": {
      "ios": {
        "appleId": "shahariyarkhan123@gmail.com",
        "ascAppId": "6753021182"
      }
    },
    "production": {
      "ios": {
        "appleId": "shahariyarkhan123@gmail.com",
        "ascAppId": "6753021182"
      }
    }
  }
}
